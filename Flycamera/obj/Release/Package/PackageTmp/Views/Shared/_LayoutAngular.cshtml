@using Flycamera.App_Start
<!DOCTYPE html >
<html ng-app="myApp">
<head>
    @Html.Partial("_MetaSEO")
    <title>@ViewBag.Title</title>
    @RenderSection("styleSheet", required: false)
    @Styles.Render("~/Content/AngularCSS")
</head>
<body class="">
    <div class="wrapper-out">
        <header>
            <div class="wrapper">
                <ul class="header__nav">
                    <li>
                        <a href="/" title="Trang chủ" class="header__nav--logo">Trang chủ</a>
                    </li>
                    <li>
                        <a href="/" title="Trang chủ" class="header__nav--home">Trang chủ</a>
                    </li>
                    <li>
                        <a href="/dich-vu/index" title="Service">Service</a>
                    </li>
@*                    <li>*@
@*                        <a href="javascript:;" title="Gallery">Gallery</a>*@
@*                    </li>*@
                    <li>
                        <a href="javascript:;" title="Price Quotation">Price Quotation</a>
                    </li>
                    <li>
                        <a href="/dich-vu/teams" title="Equipment & Team">Equipment & Team</a>
                    </li>
                    <li>
                        <a href="/dich-vu/client" title="Clients">Clients</a>
                    </li>
                    <li>
                        <a href="/dich-vu/contactus" title="Contact">Contact</a>
                    </li>
                </ul>
            </div>
        </header>


        @RenderSection("bodyContent", required: false)
        @RenderSection("contactBlock", required: false)

        <section class="wrapper__footer">
            <div class="wrapper">
                <div class="copyright-content">
                    <p>
                        Copyright &copy; 2014 Quay Phim Trên Không Flycam , Helicam , Multicopter Pro. All rights reserved.
                    </p>
                    <a class="login-bottom" href="#">Login</a>
                </div>
                <div class="frm-flowus">
                    <div class="flow-right">
                        <span>Follow us</span>
                        <a class="flow flow-ytb" title="youtube" href="#">youtube</a>
                        <a class="flow flow-fb" title="facebook" href="#">facebook</a>
                        <a class="flow flow-twt" title="twitter" href="#">twitter</a>
                    </div>
                </div>
            </div>
        </section>
        @RenderBody()
    </div>

    <script src="@Scripts.Url("/Scripts/jquery-1.11.2.js")" type="text/javascript"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.3.14/angular.min.js"></script>
    @RenderSection("scripts", required: false)
    @Scripts.Render("~/Scripts/AngularJS")
</body>
</html>
